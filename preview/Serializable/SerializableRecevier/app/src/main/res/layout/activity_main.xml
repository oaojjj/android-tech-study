<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/tv_serializable_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Serializable Data"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_serializable_data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="null"
        app:layout_constraintEnd_toEndOf="@+id/tv_serializable_title"
        app:layout_constraintStart_toStartOf="@+id/tv_serializable_title"
        app:layout_constraintTop_toBottomOf="@+id/tv_serializable_title" />

    <TextView
        android:id="@+id/tv_parcelable_data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="null"
        app:layout_constraintEnd_toEndOf="@+id/tv_parcelable_title"
        app:layout_constraintStart_toStartOf="@+id/tv_parcelable_title"
        app:layout_constraintTop_toBottomOf="@+id/tv_parcelable_title" />

    <TextView
        android:id="@+id/tv_parcelable_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Parcelable Data"
        app:layout_constraintEnd_toEndOf="@+id/tv_serializable_title"
        app:layout_constraintStart_toStartOf="@+id/tv_serializable_title"
        app:layout_constraintTop_toBottomOf="@+id/tv_serializable_data" />


</androidx.constraintlayout.widget.ConstraintLayout>